    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DW_Server_RapidCMI</DataSourceName>
        <CommandText>SELECT Count(Contact_ID) as Contacts,'Semantic' as Source
  FROM [Semantic].[mental_health].[FactContact]
 -- where Subcentre_Key&lt;0
 UNION 
   SELECT Count(Contact_ID) as Contacts,'Staging' as Source
  FROM [RapidCMI].[staging].[Contact]</CommandText>
